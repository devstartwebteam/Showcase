<!DOCTYPE html>
<html lang="en">
@Html.Partial("_Head")
    <body>
        @Html.Partial("_Header")

        <div id="s-body" class="container-fluid body-content">
            <div class="row">
                <div class="col-md-12 s-no-padding">
                    @RenderBody()
                </div>
            </div>
        </div>

        @Html.Partial("_Footer")
        @RenderSection("scripts", required: false)
    </body>
</html>
