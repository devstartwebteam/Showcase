@using Showcase.Helpers
@model IEnumerable<Showcase.ViewModels.InstagramImage>

<div class="single-footer-widget instafeed">
    <h6 class="footer_title">Instagram Feed</h6>
    <ul class="list instafeed d-flex flex-wrap">
        <li>
        @if (Model != null)
        {
            foreach (var image in Model)
            {
                @Html.InstagramImage(image.Image)
            }
        }
        </li>

        <li><img src="~/Content/images/i1.jpg" alt=""></li>
        <li><img src="~/Content/images/i2.jpg" alt=""></li>
        <li><img src="~/Content/images/i3.jpg" alt=""></li>
        <li><img src="~/Content/images/i4.jpg" alt=""></li>
        <li><img src="~/Content/images/i5.jpg" alt=""></li>
        <li><img src="~/Content/images/i6.jpg" alt=""></li>
        <li><img src="~/Content/images/i7.jpg" alt=""></li>
        <li><img src="~/Content/images/i8.jpg" alt=""></li>
    </ul>
</div>